{
  "name": "consumable-alchemy-game",
  "version": "1.0.0",
  "description": "Gamified interface for checking consumable safety and interactions - like alchemy but for human body",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p ${PORT:-3000}",
    "dev:qa": "next dev -p ${QA_PORT:-3001}",
    "dev:api": "next dev -p ${API_PORT:-3002}",
    "dev:backend": "next dev -p ${BACKEND_PORT:-3003}",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "start:qa": "next start -p ${QA_PORT:-3001}",
    "start:api": "next start -p ${API_PORT:-3002}",
    "start:backend": "next start -p ${BACKEND_PORT:-3003}",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:qa": "jest __tests__/qa/",
    "test:integration": "jest __tests__/qa/integration.test.ts",
    "test:20-sources": "jest __tests__/qa/20-sources.test.ts",
    "test:background": "node scripts/qa-background-tests.js",
    "test:all": "npm run test && npm run test:qa",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "postbuild": "next-sitemap",
    "check": "npm run type-check && npm run lint && npm run test",
    "precommit": "npm run check"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.6.0",
    "react-intersection-observer": "^9.5.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.3",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "puppeteer": "^24.22.1"
  },
  "keywords": [
    "gamification",
    "food-safety",
    "drug-interactions",
    "health-education",
    "alchemy-game",
    "gen-z",
    "react",
    "typescript"
  ],
